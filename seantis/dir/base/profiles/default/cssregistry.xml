<?xml version="1.0"?>
<object name="portal_css" meta_type="Stylesheets Registry">

    <stylesheet id="++resource++seantis.dir.base.stylesheets/token-input.css"
        enabled="True"
        media="screen"
    />
    
    <stylesheet id="++resource++seantis.dir.base.stylesheets/directory.css"
        enabled="True"
        media="all"
        expression="python: object.restrictedTraverse('@@plone_interface_info').provides('seantis.dir.base.interfaces.IDirectoryRoot')"
    />
    
    <stylesheet id="++resource++seantis.dir.base.stylesheets/search.css"
        enabled="True"
        media="all"
        expression="python: object.restrictedTraverse('@@plone_interface_info').provides('seantis.dir.base.interfaces.IDirectoryRoot')"
    />
    
    <stylesheet id="++resource++seantis.dir.base.stylesheets/print.css" 
        enabled="True"
        media="print"
        expression="python: object.restrictedTraverse('@@plone_interface_info').provides('seantis.dir.base.interfaces.IDirectoryRoot')"
    />
</object>
